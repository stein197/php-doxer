{
	"name": "stein197/doxer",
	"description": "Simple phpdoc comment parser",
	"type": "library",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Stein197\\Doxer\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Stein197\\Doxer\\": [
				"src/", "tests/"
			]
		}
	},
	"require": {
		"php": ">=7.4"
	},
	"scripts": {
		"test": "phpunit"
	},
	"scripts-descriptions": {
		"test": "Run unit tests"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.5"
	}
}
